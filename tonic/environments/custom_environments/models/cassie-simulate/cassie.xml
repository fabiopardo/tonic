<!-- Cassie simulation model developed by the Dynamic Robotics Laboratory -->
<mujoco model='cassie'>
  <compiler inertiafromgeom='auto' angle='degree' eulerseq='zyx' meshdir='./tonic/environments/custom_environments/models/cassie-simulate/cassie-stl-meshes'/>
  <size nuser_actuator='1' nuser_sensor='1' nuser_geom='1'/>

  <option timestep="0.0025"/>
